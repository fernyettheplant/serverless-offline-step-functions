{
  "compilerOptions": {
    "lib": [
      "es2018"
    ],
    "module": "commonjs",
    "target": "es2018",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": false,
    "strictNullChecks": true,
    "noImplicitAny": true
  }
}